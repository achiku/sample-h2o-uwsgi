max-connections: 1024
num-threads: 4
user: isucon_admin

access-log: /var/log/uwsgi/access.log
error-log: /var/log/uwsgi/error.log
http2-reprioritize-blocking-assets: ON
pid-file: /tmp/h2o.pid

hosts:
  default:
    listen:
      port: 80
    paths:
      "/":
        proxy.reverse.url: http://localhost:8080/
        proxy.preserve-host: ON
      "/assets":
        file.dir: /home/isucon_admin/test/assets
